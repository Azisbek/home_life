{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["components/*"],
      "@utils/*": ["utils/*"],
      "@pageLayout/*": ["pageLayout/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/catalog/index.js",
    "pageLayout/CatalogPage/components/CatalogProducts/index.js",
    "pageLayout/CatalogPage/index.jsx",
    "components/NavigateBar/components/icon/CatalogIcon.jsx",
    "components/NavigateBar/components/icon/HomeIcon.jsx",
    "components/MobileHeader/index.js"
, "components/BurgetMenu/index.js", "components/ui/Skeleton/index.jsx", "utils/generateProductSkeleton.js"  ],
  "exclude": ["node_modules"]
}
